<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="55">
<select id="select1" resultType="dto.DTOClass">
SELECT * FROM USER;
</select>

<insert id="insert1" parameterType="dto.DTOClass">
	INSERT INTO USER(uid, uname, unum) value(#{uid}, #{uname}, #{unum})
</insert>
</mapper>